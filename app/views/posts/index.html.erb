<center><br>
<h1 class= 'index'>EOWYN</h1></center>
<h2 class= 'index' id='new'>Entertaining News</h2>
<table>
  <thead>
    <tr>
      <th class= 'index' id='next'>Posts Below:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.entry %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td>

          <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br><br><br><br><br><br><br><br><br><br><br><br>

<font size="3" class= 'index' id= 'now'><%= link_to 'New Post', new_post_path %></font>
